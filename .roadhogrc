{
  "entry": "src/index.js",
  "proxy": {
    "/api": {
      "target": "http://oa.app/api/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    },
    "/": {
      "target": "http://oa.app/",
      "changeOrigin": true,
      "pathRewrite": { "^/" : "" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    }
  }
}
